upstream = "http://mirror.rit.edu/gnu/readline/?C=M&O=D"

[[channels]]
name = "release"
fetch = '''cr | grep $name | cut -d\" -f8 | vtrim -E -e 's,.tar.gz.*$,,' | grep -Ev 'diff|doc|patches|a$|<' | vfs | vsort'''

[[channels]]
name = "unstable"
fetch = '''cr | grep $name | cut -d\" -f8 | vtrim -E -e 's,.tar.gz.*$,,' | grep -Ev 'diff|doc|patches|a$|<' | vsort'''
