upstream = "openjdk/jdk"

[[channels]]
name = "unstable"
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' | vsort'''

[[channels]]
# fetch jdk release for current year
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' | grep "^$(date +%y)\." | grep -v ga | rev | sed 's,\.,u.,' | rev | vsort'''
name = "release"
expected = '''^[0-9]{2}\.u[0-9]+$'''

[[channels]]
name = "21"
upstream = "openjdk/jdk21u"
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' | grep "^21\." | grep -v ga | v3grep | rev | sed 's,\.,u.,' | rev | vsort'''
expected = '''^21\.[0-9]+\.[0-9]+\.u[0-9]+$'''

[[channels]]
name = "17"
upstream = "openjdk/jdk17u"
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' | grep "^17\." | grep -v ga | v3grep | rev | sed 's,\.,u.,' | rev | vsort'''
expected = '''^17\.[0-9]+\.[0-9]+\.u[0-9]+$'''

[[channels]]
name = "11"
upstream = "openjdk/jdk11u"
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' | grep "^11\." | grep -v ga | v3grep | rev | sed 's,\.,u.,' | rev | vsort'''
expected = '''^11\.[0-9]+\.[0-9]+\.u[0-9]+$'''

[[channels]]
name = "8"
upstream = "openjdk/jdk8u"
fetch = '''gr | vtrim -e 's,^jdk,,' -e 's,-,.,' -e 's,\+,.,' -e 's,b,,' -e 's,u,.,' | grep -v ga | vsort | rev | sed 's,\.,u.,' | rev'''
expected = '''^8\.[0-9]+\.u[0-9]+$'''
